
{
  "name": "printeasy-backend",
  "version": "2.0.0",
  "description": "PrintEasy Backend API v2.0 - Connecting customers with local print shops through digital ordering and file management",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "db:seed": "node seeders/comprehensive-test-data.js",
    "db:reset": "node -e \"require('./models').sequelize.sync({force: true}).then(() => {console.log('Database reset complete'); process.exit()})\"",
    "db:setup": "npm run db:reset && npm run db:seed",
    "test": "echo \"API testing available via Postman collection - import postman-collection.json\"",
    "health": "curl http://localhost:3001/health || echo 'Server not running'",
    "logs": "tail -f logs/app.log 2>/dev/null || echo 'No log file found'",
    "postman": "echo \"Import backend/postman-collection.json into Postman for complete API testing\"",
    "validate": "node -e \"console.log('âœ… Package.json is valid')\"",
    "production": "NODE_ENV=production node app.js"
  },
  "keywords": [
    "printing",
    "api",
    "nodejs", 
    "express",
    "sequelize",
    "postgresql",
    "phone-login",
    "file-upload",
    "real-time-chat",
    "order-management",
    "role-based-auth",
    "cors-enabled",
    "postman-ready"
  ],
  "author": "PrintEasy Development Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.35.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/printeasy/backend.git"
  },
  "bugs": {
    "url": "https://github.com/printeasy/backend/issues"
  },
  "homepage": "https://github.com/printeasy/backend#readme"
}
